// prisma/schema.prisma
datasource db {
  provider = "postgresql"
  url       = env("POSTGRES_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

generator client {
  provider = "prisma-client-js"
}

model Outputs {
  id     Int    @id @default(autoincrement())
  name   String
  board  Int
  gpio   Int
  state  Int
}

model Boards {
  id            Int      @id @default(autoincrement())
  board         Int
  last_request  DateTime @default(now()) @updatedAt
}
